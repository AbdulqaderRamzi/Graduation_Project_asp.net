<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>DjangoDotNet</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/Graduation_Project.styles.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/css/khaled.css" />
    <link rel="stylesheet" href="~/css/ok.css" />


</head>
<body>
    <header>
        <nav class="navbar navbar-expand-sm navbar-toggleable-sm navbar-light bg-white box-shadow mb-0" style="color:#417690; border-bottom: 2px solid #417690;">
            <div class="container-fluid">
                <a style="font-weight:1000; color:#417690;" class="navbar-brand custom-color" asp-area="Customer" asp-controller="Home" asp-action="Index">Django.Net</a>
                <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target=".navbar-collapse" aria-controls="navbarSupportedContent"
                        aria-expanded="false" aria-label="Toggle navigation">
                    <span class="navbar-toggler-icon"></span>
                </button>
                <div class="navbar-collapse collapse d-sm-inline-flex justify-content-between">
                    <ul class="navbar-nav flex-grow-1">
                        <li class="nav-item">
                            <a style="color:#417690;" class="custom-color nav-link" asp-area="Customer" asp-controller="Home" asp-action="Index">Home</a>
                        </li>
                        <li class="nav-item">
                            <a style="color:#417690;" class="custom-color nav-link" asp-area="Customer" asp-controller="Donation" asp-action="Create">Donation</a>
                        </li>
                        <li class="nav-item">
                            <a style="color:#417690;" class="custom-color nav-link" asp-area="Customer" asp-controller="Community" asp-action="Index">Community</a>
                        </li>
                        <li class="nav-item">
                            <a style="color:#417690;" class="custom-color nav-link" asp-area="Customer" asp-controller="Inquiry" asp-action="Create">Inquiry</a>
                        </li>
                        <li class="nav-item">
                            <a style="color:#417690;" class="custom-color nav-link" asp-area="Customer" asp-controller="Home" asp-action="ApplyTojob">career</a>
                        </li>
                        <li class="nav-item">
                            <a style="color:#417690;" class="custom-color nav-link" asp-area="Customer" asp-controller="Subscription" asp-action="Subscriptions">Packages</a>
                        </li>
                        <li class="nav-item">
                            <a style="color:#417690;" class="custom-color nav-link" asp-area="Customer" asp-controller="ContactUs" asp-action="Contactus">Contact us</a>
                        </li>
                        <li class="nav-item">
                            <a class="custom-color nav-link" asp-area="Customer" asp-controller="ChatRoom" asp-action="UserChatRoom">
                                <svg xmlns="http://www.w3.org/2000/svg" width="20" height="20" fill="#417690" class="bi bi-chat-dots-fill" viewBox="0 0 16 16">
                                    <path d="M16 8c0 3.866-3.582 7-8 7a9.06 9.06 0 0 1-2.347-.306c-.584.296-1.925.864-4.181 1.234-.2.032-.352-.176-.273-.362.354-.836.674-1.95.77-2.966C.744 11.37 0 9.76 0 8c0-3.866 3.582-7 8-7s8 3.134 8 7zM5 8a1 1 0 1 0-2 0 1 1 0 0 0 2 0zm4 0a1 1 0 1 0-2 0 1 1 0 0 0 2 0zm3 1a1 1 0 1 0 0-2 1 1 0 0 0 0 2z" />
                                </svg>
                            </a>
                        </li>
                        <partial name="_AdminPartial" />
                    </ul>
                    <partial name="_LoginPartial" />
                </div>
            </div>
        </nav>
    </header>
    <div class="d-flex">
        <partial name="_Sidebar" />
        <div class="container" style="width: 80%">
            <main role="main" class="pb-3">
                @RenderBody()
            </main>
        </div>
    </div>

@*     <footer class="border-top footer text-muted">
        <div class="container">
            &copy; 2023 - khalod.zeko@gmail.com
        </div>
    </footer> *@
    <script src="https://cdn.tiny.cloud/1/59ie8nq0s0osmyghcbzxvqxum8jw8gnedrtufptnw9fv2n6q/tinymce/6/tinymce.min.js" referrerpolicy="origin"></script>
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="https://code.jquery.com/jquery-3.3.1.slim.min.js" integrity="sha384-q8i/X+965DzO0rT7abK41JStQIAqVgRVzpbzo5smXKp4YfRvH+8abtTE1Pi6jizo" crossorigin="anonymous"></script>
    <script src="https://cdn.jsdelivr.net/npm/popper.js@1.14.3/dist/umd/popper.min.js" integrity="sha384-ZMP7rVo3mIykV+2+9J3UJ46jBk0WLaUAdn689aCwoqbBJiSnjAK/l8WvCWPIPm49" crossorigin="anonymous"></script>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@4.1.3/dist/js/bootstrap.min.js" integrity="sha384-ChfqqxuZUCnJSK3+MXmPNIyE6ZbWh2IMqE241rYiqJxyMiZ6OW/JmZQ5stwEULTy" crossorigin="anonymous"></script>
    <script src="//cdn.datatables.net/1.13.3/js/jquery.dataTables.min.js" asp-append-version="true"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    <script src="https://cdn.tiny.cloud/1/g28lhnxtlihu4l4a381gw7tz47voznshwbi10lmsvlekrfme/tinymce/6/tinymce.min.js" referrerpolicy="origin"></script>
    <script src="//cdnjs.cloudflare.com/ajax/libs/toastr.js/latest/js/toastr.min.js"></script>
    <script src="https://code.jquery.com/jquery-3.6.0.min.js"></script>
    <script src="~/js/ok.js"></script>
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>